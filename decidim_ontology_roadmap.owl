    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#Participant"/>
        <rdfs:comment xml:lang="en">A participant corresponding to a registered account in the platform.</rdfs:comment>
        <rdfs:label xml:lang="en">Registered participant</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/#VerifiedParticipant -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/#VerifiedParticipant">
