        <owl:inverseOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/specifies"/>
        <rdfs:comment xml:lang="en">General property that indicates that some aspect or attribute of the entity is specified by an other one.</rdfs:comment>
        <rdfs:label xml:lang="en">specified by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/specifies -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/specifies">
        <rdfs:comment xml:lang="en">General property that indicates that an entity specifies an aspect or attribute of another entity, such as its Scope, Category or Role.

It is inverse property of specified_by.</rdfs:comment>
        <rdfs:label xml:lang="en">specifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/subcategory_of -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/subcategory_of">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/belong_to"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/Category"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Category"/>
        <rdfs:label xml:lang="en">subcategory of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/subgroup_of -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/subgroup_of">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/belong_to"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/Group"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Group"/>
        <rdfs:label xml:lang="en">subgroup of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/supporter_of -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/supporter_of">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/acts_on"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/VerifiedPerson"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
        <rdfs:label xml:lang="en">supporter of</rdfs:label>
        <rdfs:label>supports</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/votes_comment -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/votes_comment">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/acts_on"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Comment"/>
        <rdfs:label>votes comment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/withdrawn_by -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/withdrawn_by">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_actor"/>
        <owl:inverseOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/withdraws"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
        <rdfs:label xml:lang="en">withdrawn by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/withdraws -->

    <owl:ObjectProperty rdf:about="http://ontology.decidim.org/decidim_roadmap/withdraws">
        <rdfs:subPropertyOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/acts_on"/>
        <rdfs:domain rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
        <rdfs:range rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
        <rdfs:label xml:lang="en">withdraws</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ontology.decidim.org/decidim_roadmap/AccountabilityContainer -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/AccountabilityContainer">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Result"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This functionality allows participants to monitor the results of a participation space, and the progress of derived projects.

It is a ParticipationFunctionality of kind Container, and includes Items of kind Result, vinculated to the corresponding accepted Proposals and to projects deriving from them.</rdfs:comment>
        <rdfs:label xml:lang="en">Accountability container</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Administrator -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Administrator">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Moderator"/>
        <rdfs:comment xml:lang="en">A registered participant who has the role of Administrator on a ParticipationArtifact can configure and manage it, and can grant other RegisteredParticipants administration permissions of Collaborator, Moderator or Administrator in the same ParticipationArtifact or on a ParticipationArtifact contained in it.</rdfs:comment>
        <rdfs:label xml:lang="en">Administrator</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Amendment -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Amendment">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/amendment_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An Amendment is a kind of Proposal that refers to a specific Proposal it proposes to enmend, to which it is linked through the property amendment_of.</rdfs:comment>
        <rdfs:label xml:lang="en">Amendment</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Assembly -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Assembly">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Group"/>
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/SovereignOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Consultation"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Initiative"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Process"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProcessGroup"/>
        <rdfs:comment xml:lang="en">An assembly is a participation space, defined as a structured group of participants that meet periodically, individually or representatively, and can create proposals and make decisions. Beyond being a Participation space, an assembly is also a Group, and as such it can be the promoter of a Process, or participate in other participation spaces.

An Assembly may have WorkingGroups, i.e. subgroups created to work on specific issues.</rdfs:comment>
        <rdfs:label xml:lang="en">Assembly</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Attribute -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Attribute">
        <rdfs:comment xml:lang="en">General class for entities that represent an attribute of other entities, such as their Scope or Category.</rdfs:comment>
        <rdfs:label xml:lang="en">Attribute</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/AuthoredItem -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/AuthoredItem">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_creator"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class represents Items that have an author. While some content in the Platform and ParticipationSpaces is created by the Adinistrators, AuthoredItems have a creator that can typically be any RegisteredParticipant.</rdfs:comment>
        <rdfs:label xml:lang="en">Authored resource</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Blog -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Blog">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Blog_post"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Thread"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Blog allows for the reation of pieces of content (BlogPosts) in shown in chronological order and classified according to the patform&apos;s content categorization system.

A Blog is a ParticipationFunctionality of kind Container, that includes Items of kind BlogPost. Blog posts can in turn receive comments and conversation Threads, analogously to Proposals and other Items.</rdfs:comment>
        <rdfs:label xml:lang="en">Blog</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Blog_post -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Blog_post">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_container"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/Blog"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A BlogPost is a piece of content that is published within a Blog.</rdfs:comment>
        <rdfs:label xml:lang="en">Blog post</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/BudgetedProject -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/BudgetedProject">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/AuthoredItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_container"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipativeBudget"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_creator"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A kind of proposal, that has a budget associated with it.</rdfs:comment>
        <rdfs:label xml:lang="en">Budgeted project</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Category -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Category">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Attribute"/>
        <rdfs:comment xml:lang="en">Categories are used to group items according to some criteria, typically the topic. They help participants browse and search information in a Participation space.</rdfs:comment>
        <rdfs:label xml:lang="en">Category</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Collaborator -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Collaborator">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Role"/>
        <rdfs:comment xml:lang="en">A RegisteredParticipant having the Role of Collaborator in a given role scope can access the same information as an Administrator, including configuration panels, but cannot modify it.</rdfs:comment>
        <rdfs:label xml:lang="en">Collaborator</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Comment -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Comment">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/AuthoredItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_creator"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/reply_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Comments are ParticipationItems authored by RegisteredParticipants, that can be associated with different ParticipationFunctionalities and Items. 

A Comment can be referred to another comment (NestedComment) or to some other ParticipationItem (FirstLevelComment).</rdfs:comment>
        <rdfs:label xml:lang="en">Comment</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Configurator -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Configurator">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/configurator_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Administration panel associated to a ParticipationArtifact, that allows Administrators to configure it. Collaborators of the artifat can access the configurator but not modify it; moderators can also access the configurator, and additionally they can manage certain aspects such as accepting or rejecting proposals.
A Configurator is linked to the ParticipationArtifacts it allows to configure through the property configurator_of.</rdfs:comment>
        <rdfs:label xml:lang="en">Configurator</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Consultation -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Consultation">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/SovereignOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Initiative"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Process"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProcessGroup"/>
        <rdfs:comment xml:lang="en">A Consultation is a ParticipationSpace that allows a votation on some issue, involving all the participants of the SovereignOrganization promoting it. Participants can not only vote, but also get information, debate and monitor the results.</rdfs:comment>
        <rdfs:label xml:lang="en">Consultation</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Container -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Container">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality"/>
        <rdfs:comment xml:lang="en">Functionalities of this kind contain ParticipationItems, to which they are linked to the property container_of. 

Different kinds of ParticipationContainers contain different kinds of ParticipationItems: for example, a Debate contains items of ind Thread and Comment, a ProposalIncubator contains Proposals, a ParticipativeBudget contains BudgetedProposals and a MeetingCalendar contains Meetings.</rdfs:comment>
        <rdfs:label xml:lang="en">Container</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Debate -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Debate">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Blog_post"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Thread"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This functionality allows for the creation of debates about specific questions, defined by the Administrators or by RegisteredParticipants.

A Debate is a ParticipationFunctionality of kind Container, that includes Items of kind Comment, and the corresponding Threads.</rdfs:comment>
        <rdfs:label xml:lang="en">Debate</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/FirstLevelComment -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/FirstLevelComment">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Comment"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/NestedComment"/>
        <rdfs:comment xml:lang="en">A comment referred to some ParticipationItem, which is not another comment.</rdfs:comment>
        <rdfs:label xml:lang="en">First level comment</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Group -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Group">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Organization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_member"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/RegisteredPerson"/>
        <rdfs:comment xml:lang="en">A collective account corresponding to a group of registered participants.</rdfs:comment>
        <rdfs:label xml:lang="en">Group</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Initiative -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Initiative">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/#VerifiedParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/initiative_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Process"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProcessGroup"/>
        <rdfs:comment xml:lang="en">While a Process is necessarily promoted by an organization, an Initiative is a ParticipationSpace that can be promoted by any VerifiedParticipant, i.e. by any individual citizen or organization whose account has been verified to correspond to their real identity. 

An Initiative is always connected to a Proposal it promotes, with the goal of gathering support for it, and to be considered for acceptance by the SovereignOrganization running the ParticipationPlatform.</rdfs:comment>
        <rdfs:label xml:lang="en">Initiative</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ManagedAccount -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ManagedAccount">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/VerifiedPerson"/>
        <rdfs:comment xml:lang="en">A ManagedAccount is an account managed on behalf of another Person, that for some reason is not able to manage it by themselves.</rdfs:comment>
        <rdfs:label xml:lang="en">Managed account</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Meeting -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Meeting">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_attendee"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Meeting is a physical gathering in which RegisteredParticipants can meet presentially to discuss about some issue and create Proposals. 

A Meeting is linked to the participants attending it through the property has_attendee.

A Meeting can be included in a Workshop or in an EventCalendar associated to some ParticipationSpace.</rdfs:comment>
        <rdfs:label xml:lang="en">Meeting</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/MeetingCalendar -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/MeetingCalendar">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Meeting"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A MeetingCalendar is a group of meetings associated to a given ParticipationSpace. 

Like a Workshop, it is a ParticipationFunctionality of kind Container, that includes Item of kind Meeting; however, in this case events may typically span over a long period of time, be located in different places and not be related to each other.</rdfs:comment>
        <rdfs:label xml:lang="en">Meeting calendar</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Moderator -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Moderator">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Collaborator"/>
        <rdfs:comment xml:lang="en">The role of Moderator attributes to a RegisteredParticipant the same rights as a Collaborator for accessing information, plus the possibility of managing ParticipationArtifacts within the role&apos;s scope (for example, they can assign a status to a Proposal, and provide an official response).</rdfs:comment>
        <rdfs:label xml:lang="en">Moderator</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/NestedComment -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/NestedComment">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Comment"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/reply_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/Comment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A comment that replies to another comment.</rdfs:comment>
        <rdfs:label xml:lang="en">Nested comment</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Newsletter -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Newsletter">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality"/>
        <rdfs:comment xml:lang="en">A Newsletter allows to broadcast messages to all the RegisteredParticipants associated to a ParticipationPlatform or to a specific ParticipationSpace.</rdfs:comment>
        <rdfs:label xml:lang="en">Newsletter</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/OfficialAccount -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/OfficialAccount">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/VerifiedPerson"/>
        <rdfs:comment xml:lang="en">A verified person whose account name has also been verified to correspond to the real name.</rdfs:comment>
        <rdfs:label>Official account</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Organization -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Organization">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#Agent"/>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/Person"/>
        <rdfs:comment xml:lang="en">An organized group of people with a collective purpose. Specific subclasses represent the case of groups of registered participants (Group), and the organization promoting a ParticipationPlatform or a ParticipationSpace (Sovereign organization).</rdfs:comment>
        <rdfs:label xml:lang="en">Organization</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Page -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Page">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality"/>
        <rdfs:comment xml:lang="en">This ParticipationFunctionality allows to create informative pages with enriched text, images or videos.</rdfs:comment>
        <rdfs:label xml:lang="en">Page</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact">
        <rdfs:comment xml:lang="en">General class including the various elements that allow democratic participation in Decidim: ParticipationPlatforms, ParticipationSpaces (such as Participation processes or Assemblies), ParticipationFunctionalities (such as Debates, Surveys or Votations), and ParticipationItems (such as individual proposals, meetings or comments).</rdfs:comment>
        <rdfs:label xml:lang="en">Artifact</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/functionality_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_configurator"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Configurator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class includes the different functionalitites that can be added to a ParticipationSpace by RegisteredParticipants having administrative role to configure it. 

Each ParticipationFunctionality has to be linked to at least one participationSpace it belongs to, through the property functionality_of.
Furthermore, a ParticipationFunctionality needs to have a Configurator that allows its configuration by participants having the permissions to do so.

Some functionalitites represent one single element that is added to the ParticipationSpace, such as a Page, a Votation or a Survey, while other functionalities are elements that contain participationItems, such as Proposals, BudgetedProposals, Meetings or Comments; such functionalitties that contain ParticipationItems are defined as ParticipationContainers.

While some functionalities are general and can belong to any kind of ParticipationSpace, such as a Page, a Blog or a Debate, other are specific to a certain kind of participationSpace. E.g., a WorkingGroup can only belong to an Assembly. In the latter case, the range of the property functionality_of for that particular ParticipationFunctionality is restrictetd to the kind of space it can belong to.</rdfs:comment>
        <rdfs:label xml:lang="en">Component</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipationItem -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipationItem">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
        <rdfs:comment xml:lang="en">This general class represents all individual items within a participation space such as Proposals, Comments, meetings, Blog posts.

Items are typically contained in some ParticipationContainer associated with a ParticipationSpace, and different kinds of Items are typically contained in different kinds of containers. For example, Bog posts in Blogs, Proposals in ProposalContainers, Meetings in Workshops or MeetingCalendars, and so on.</rdfs:comment>
        <rdfs:label xml:lang="en">Resource</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipationPlatform -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipationPlatform">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/SovereignOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This class represent instances of a participation platform. 

It can include different kinds of ParticipationSpaces, such as Assemblies, Processes, Consultations or Initiatives; all ParticipationArtifacts ultimately relate to some platform.</rdfs:comment>
        <rdfs:label xml:lang="en">Platform</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipativeBudget -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipativeBudget">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/BudgetedProject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This component allows participants to collectively define a budget by prioritizing budgeted projects.

It is a ParticipationFunctionality of kind Container, and includes Items of kind BudgetedProposal.</rdfs:comment>
        <rdfs:label xml:lang="en">Participative budget</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ParticipativeText -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ParticipativeText">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProposalContainer"/>
        <rdfs:comment xml:lang="en">This functionality allows participants to collaboratively redact a text as a collection of Proposals. Collaborative editing emerges from interaction of participants with the Proposals that compose the text. 

This functionality is a special kind of ProposalContainer, that composes proposals within a unified text, or decomposes a text into various proposals that can be discussed and prioritized separately.

A ParticipativeText is a Functionality of kind Container, that includes Items of kind Proposal.</rdfs:comment>
        <rdfs:label xml:lang="en">Participative text</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Person -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Person">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#Agent"/>
        <rdfs:comment xml:lang="en">General class that represents any person as an individual agent, opposed to collective agents (Organizations).</rdfs:comment>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Phase -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Phase">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/phase_of"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/Process"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This functionality allows the administrators of a Process to design and different phases in which the process is divided, with a start and an end date, and with different actions and permissions for each phase.</rdfs:comment>
        <rdfs:label xml:lang="en">Phase</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Process -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Process">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/SovereignOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProcessGroup"/>
        <rdfs:comment xml:lang="en">A Process is a ParticipationSpace that allows participants to interact with one another in different phases in a collective decision making effort.

A Process is promoted by a SovereignOrganization and can contain in different phases different kinds of functionalities, such as Debates, MeetingCalendars, ParticipatoryBudgets or Accountability.</rdfs:comment>
        <rdfs:label xml:lang="en">Process</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ProcessGroup -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ProcessGroup">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_promoter"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/SovereignOrganization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A GroupOfProcesses groups together different processes promoted by the same SovereignOrganization.</rdfs:comment>
        <rdfs:label xml:lang="en">Process group</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Project -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Project">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/derives_from"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Result"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A Project is an Item that represents the implementation of some Result, in turn associated with some accepted Proposal.

It is connected to some Result through the property derived_from, and Participants can monitor its state of execution.</rdfs:comment>
        <rdfs:label xml:lang="en">Project</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Promoter -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Promoter">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Role"/>
        <rdfs:comment xml:lang="en">A RegisteredParticipant can have the role of Promoter with respect to a ParticipationArtifact they promote. For example, a SovereignOrganization can be the promoter of a ParticipationPlatform or ParticipationSpace, and a RegisteredParticipant can be the Promoter of an Initiative.</rdfs:comment>
        <rdfs:label xml:lang="en">Promoter</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Proposal -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Proposal">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/AuthoredItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_container"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/ProposalContainer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_creator"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Proposals are ParticipationItems that can be authored by different kinds of RegisteredParticipants, and can receive support from other VerifiedParticipants. Proposals are moderated and can be rejected, or accepted and turned into Results and Projects.</rdfs:comment>
        <rdfs:label xml:lang="en">Proposal</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ProposalContainer -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ProposalContainer">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This functionality allows RegisteredParticipants to create Proposals and comment and support proposals created by others.

It is a ParticipationFunctionality of kind Container, and includes Items of kind Proposal.</rdfs:comment>
        <rdfs:label xml:lang="en">Proposal container</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/ProposalIncubator -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/ProposalIncubator">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Container"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/container_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A ProposalIncubator allows RegistereParticipants to create Proposal drafts, and collaborate with each other in a preliminary phase before presenting proposals for gathering support to them.

A ProposalIncubator is a Functionality of kind Container, that includes Items of kind &quot;Proposal&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">Proposal incubator</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/RegisteredGroup -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/RegisteredGroup">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
        <rdfs:label xml:lang="en">Registered group</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/RegisteredPerson -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/RegisteredPerson">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant"/>
        <rdfs:comment xml:lang="en">A RegisteredParticipant who is an individual Person, as opposed to collective accounts representing Organizations or Groups of participants.</rdfs:comment>
        <rdfs:label xml:lang="en">Registered person</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Result -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Result">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationItem"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/result_of"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/Proposal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_container"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/AccountabilityContainer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">This kind of ParticipationItem allows Participants to trace and monitor the outcomes of a participative process. 

A Result belongs to an AccountabilityContainer, within a ParticipationSpace.

Each Result is associated to one or more accepted Proposals, and to one or more Projects derived from it. Results are the conjunction between Proposals and Projects that implement them.</rdfs:comment>
        <rdfs:label xml:lang="en">Result</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Role -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Role">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Attribute"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/has_scope"/>
                <owl:someValuesFrom rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Abstract class to represent that a given participant has a certain function in the scope of some ParticipationArtifact.</rdfs:comment>
        <rdfs:label xml:lang="en">Role</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Scope -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Scope">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/Attribute"/>
        <rdfs:comment xml:lang="en">A scope is an Attribute that delimits a ParticipationSpace within a certain geographical area (TerritorialScope subclass) or topic/sector (SectorialScope).</rdfs:comment>
        <rdfs:label xml:lang="en">Scope</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/SearchEngine -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/SearchEngine">
        <rdfs:subClassOf rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontology.decidim.org/decidim_roadmap/belong_to"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://ontology.decidim.org/decidim_roadmap/ParticipationPlatform"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A SearchEngine allows participants to search for all the indexable content in a Platform or specific Process or Functionality.</rdfs:comment>
        <rdfs:label xml:lang="en">Search engine</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/SectorialScope -->

