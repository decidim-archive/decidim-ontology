        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/#Participant -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/#Participant">
