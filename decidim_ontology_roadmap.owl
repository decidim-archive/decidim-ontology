        <rdfs:comment xml:lang="en">This functionality represents a votation system associated to a ParticipationSpace.

Different votation systems are possible and can be configured by the Administrators.</rdfs:comment>
        <rdfs:label xml:lang="en">Votation</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/WorkingGroup -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/WorkingGroup">
