            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A WorkingGroup is a Group of participants that collaborate on a specific topic or goal within an Assembly, and interacts with the Assembly it belongs to.</rdfs:comment>
        <rdfs:label xml:lang="en">Working group</rdfs:label>
    </owl:Class>
    


    <!-- http://ontology.decidim.org/decidim_roadmap/Workshop -->

    <owl:Class rdf:about="http://ontology.decidim.org/decidim_roadmap/Workshop">
